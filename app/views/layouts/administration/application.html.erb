<!DOCTYPE html>
<html>
	<head>
		<title>MsFixMeApp</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
		<%= render 'layouts/shim' %>
	</head>
	<body>

		<div id='content'>
			<%= render 'layouts/administration/header' %>

			<div class="container" id='main-body'>
	      <%= yield %>
	      <div style="clear: both;">
	      <%= render 'layouts/administration/footer' %>
	    </div>
		</div>
	</body>
</html>


